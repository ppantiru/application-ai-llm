  <content>{{velocity}}
{{html clean="false"}}
&lt;div id="chatUI">
    &lt;select id="model-menu">
    &lt;/select>
    &lt;div id="chatContent">
    &lt;/div>
    &lt;div id="formContainer">
        &lt;form id="userChat" method="POST" onsubmit="return false;">
            &lt;textarea name="input" id="txt-input">&lt;/textarea>
            &lt;div id="form-control-panel">
                &lt;div class="stacked-items">
                    &lt;nav class="stacked-items">
                        &lt;select id="features-menu">
                        &lt;/select&gt;
                    &lt;/nav>
                    &lt;div class="form-button-container">
                        &lt;div class="stacked-items">
                            &lt;div class="form-button">
                                &lt;input class="button" title="Stop the current request." id="stopRqst" type="button"
                                    value="$services.localization.render('llm.ui.stop')" style="display: none;" />
                            &lt;/div>
                            &lt;div class="form-button">
                                &lt;input class="button" title="Submit your message to the selected model." id="subButton"
                                    type="submit" value="$services.localization.render('llm.ui.submit')" />
                                &lt;div id="dropDownContainer" style="position: relative;">
                                    &lt;input type="button" class="button" value="&amp;#x2630;" id="dropMenu">
                                    &lt;div class="dropdown-content" id="dropMenuContent">
                                        &lt;a id="newconv" class="dropTile" data-protected="false">$services.localization.render('llm.ui.newconv')&lt;/a>
                                        &lt;hr/>
                                        &lt;a href="/xwiki/bin/admin/XWiki/XWikiPreferences?editor=globaladmin&amp;section=AI%20LLM%20Application"
                                            id="configRedir" class="dropTile" data-protected="true">$services.localization.render('llm.ui.configRedir')&lt;/a>
                                        &lt;a href="/xwiki/bin/view/AI/PromptDB/"
                                            id="promptDBRedir" class="dropTile" data-protected="true">$services.localization.render('llm.ui.promptDBRedir')&lt;/a>
                                        &lt;hr/>
                                        &lt;a id="settingsTile" class="dropTile" data-protected="true">$services.localization.render('llm.ui.param')&lt;/a>
                                    &lt;/div>
                                &lt;/div>
                            &lt;/div>
                        &lt;/div>
                    &lt;/div>
                &lt;/div>
            &lt;/div>
    &lt;/div>
    &lt;input type="hidden" id="form_token" value="${services.csrf.token}" />
    &lt;/form>
&lt;/div>
&lt;script type="text/javascript">
    require(['xwiki-llm'], function (llm) {
        llm.init();
    });
&lt;/script>
{{/html}}
{{/velocity}}</content>
